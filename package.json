{
  "name": "customer1",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rimraf build",
    "build": "node -r babel-register ./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "node -r babel-register ./node_modules/.bin/webpack --config webpack.config.prod.js --progress --profile --colors --display-error-details --display-cached",
    "thenelectron": "sleep 5; electron build",
    "electron:prod": "NODE_ENV=production electron build",
    "electron:dev": "NODE_ENV=development electron build",
    "dev:main": "node -r babel-register ./node_modules/.bin/webpack --config webpack.config.main.js --progress --profile --colors --display-error-details --display-cached --watch",
    "dev:renderer": "node -r babel-register ./node_modules/.bin/webpack-dev-server --config webpack.config.renderer.js --inline --hot --progress",
    "start": "NODE_ENV=development npm-run-all --parallel dev:main dev:renderer thenelectron",
    "test": "node -r babel-register ./node_modules/.bin/jest"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "devtron": "^1.4.0",
    "electron": "^9.1.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.5.7",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-validator": "^2.3.0",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "file-url": "^2.0.2",
    "litening": "github:untoldwind/litening",
    "mime": "^1.3.4",
    "preact": "^7.2.0",
    "preact-redux": "^2.0.0",
    "promise-monofill": "^1.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-electron-store": "^0.4.1",
    "redux-promise": "^0.5.3",
    "sharp": "^0.17.2",
    "thenify": "^3.2.1",
    "thenify-all": "^1.6.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": ".*-test.js",
    "testEnvironment": "node"
  }
}
